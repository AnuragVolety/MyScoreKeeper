<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.android.myscorekeeper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".TableTennis"
            android:label="TableTennis">
        </activity>
        <activity
            android:name=".LawnTennis"
            android:label="LawnTennis">
        </activity>
        <activity
            android:name=".Volleyball"
            android:label="Volleyball">
        </activity>
        <activity
            android:name=".B1s"
            android:label="Singles Playing 1 Set">
        </activity>
        <activity android:name=".Match">
        </activity>
    </application>

</manifest>